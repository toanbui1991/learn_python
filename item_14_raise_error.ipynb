    "def divide(a, b):\n",
    "    try:\n",
    "        return a / b\n",
    "    except ZeroDivisionError as e:\n",
    "        print('Error: ', e)\n",
    "        raise ValueError('Invalid inputs')\n",
    "\n",
    "x, y = 5, 0\n",
    "try:\n",
    "    result = divide(x, y)\n",
    "except ValueError as e:\n",
    "    print(e)\n",
    "else:\n",
    "    print('Result is {:.2f}'.format(result))"
   ]
