    "#use slice operator like slice(start, stop, [step])\n",
    "test_list = list(range(10))\n",
    "print(test_list)\n",
    "data = test_list[:4] #first 4 element\n",
    "print(data)\n",
    "data = test_list[-2:] #last 2 elment\n",
    "print(data)\n",
    "data = test_list[2:5] #return [2, 3, 4]\n",
    "print(data)\n",
    "data = test_list[::2]\n",
    "print(data)"
